{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "KeyStone": "Debug" // Set to Debug to get more detailed logs
    }
  },
  "AdSettings": {
    "ForestRootDomain": "lab.local",
    "Domains": [
      "lab.local",
      "new.lab.local"
    ],
    "AccessControl": {
      "GeneralAccessGroups": [ "L2" ],
      "HighPrivilegeGroups": [ "L3", "Domain Admins" ]
    },
    "Provisioning": {
      "DefaultUserOuFormat": "OU=Users,OU=_Managed,{domain-components}",
      "AdminUserOuFormat": "OU=_AdminAccounts,{domain-components}",
      "OptionalGroupsForStandard": [ "OptionalGroup1", "OptionalGroup2" ],
      "OptionalGroupsForHighPrivilege": [ "OptionalGroup1", "OptionalGroup2" ],
      "AdminGroup": "Domain Admins",
      "SearchBaseOus": [
        "OU=Users,OU=_Managed,DC=lab,DC=local",
        "OU=Users,OU=_Managed,DC=new,DC=lab,DC=local"
      ]
    }
  }
}
