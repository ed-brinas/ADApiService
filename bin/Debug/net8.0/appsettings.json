{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "KeyStone": "Debug" // Set to Debug to get more detailed logs
    }
  },
  "AdSettings": {
    "ForestRootDomain": "ncc.local",
    "Domains": [
      "ncc.local"
    ],
    "AccessControl": {
      "GeneralAccessGroups": [ "L2" ],
      "HighPrivilegeGroups": [ "L3", "Domain Admins" ]
    },
    "Provisioning": {
      "DefaultUserOuFormat": "OU=Users,OU=_Managed,{domain-components}",
      "AdminUserOuFormat": "OU=_AdminAccounts,{domain-components}",
      "OptionalGroupsForStandard": [ "L1", "L2","EMS-RDP", "FEP-RDP" ],
      "OptionalGroupsForHighPrivilege": [ "L3", "Domain Admins" ],
      "SearchBaseOus": [
        "OU=Users,OU=_Managed,{domain-components}"      
      ]
    }
  }
}
